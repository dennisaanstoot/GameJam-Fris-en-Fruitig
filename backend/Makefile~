
CC=clang

LFLAGS= -ljson -lwebsockets -lm

all: server

server: list.o entity.o json.o game.o main.o
	$(CC) list.o entity.o game.o main.o $(LFLAGS) -o server -g

main.o: main.c
	$(CC) -g -c main.c

entity.o: entity.c
	$(CC) -g -c entity.c

list.o: list.c
	$(CC) -g -c list.c

json.o: json.c
	$(CC) -g -c json.c

game.o: game.c
	$(CC) -g -c game.c

clean:
	rm *.o server
